@{
    Layout = "~/Views/Shared/Layouts/_WizardLayout.cshtml";
}
<div id="page-content">
    <h1>@Model.ArticleTitle</h1>
    <div id="wizardStep-content">
        <table class="WizardBlock">
            <tbody>
                <tr class="WizardBlockTop">
                    <td class="Left"></td>
                    <td class="Center"></td>
                    <td class="Right"></td>
                </tr>
                <tr class="WizardBlockMiddle">
                    <td class="Left"></td>
                    <td class="Center">

                        <div class="WizardStepList">
                            <div class="WizardListHeader">
                                <h2>Your Steps</h2>
                            </div>
                        </div>

                        <div class="WizardStepContent ">
                        
                        </div>

                    </td>
                    <td class="Right"></td>
                </tr>
                <tr class="WizardBlockBottom">
                    <td class="Left"></td>
                    <td class="Center"></td>
                    <td class="Right"></td>
                </tr>
            </tbody>
        </table>
    </div>
    <div id="FeedBackComponent">     
    </div>   
    <div id="omniture-data" class="hidden">
        <div id="wizard-tile">@Model.ArticleTitle</div>
    </div>
    @{ 
        Html.RenderPartial("~/views/Shared/Controls/AssistanceModule.cshtml"); 
    }
</div>
<script type="text/javascript">
    $(document).ready(function () {
        loadFirstWizardContent('/@System.Globalization.CultureInfo.CurrentUICulture.Name/WizardStep/GetWizardStep', "@Model.FirstStepUrl", '');
        //$('#FeedBackModuleContainer').hide();
    });
</script>