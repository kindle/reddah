    {
        <ul>
            <li id="Model.Index" class="contactUsProductCategoriesTitle">@Model.Title</li>
            @{
               var i = 0;
            }
            @foreach (var category in Model.Categories)
            {
                //if (i >= Model.MaxCategoriesLimit)
                //{
                //    break;
                //}
                <li id="@category.Index" class="contactUsCategory" link="@category.ArticleUrl">
                    @*<a onkeypress="javascript:keyClick(event, @category.NavigationUrl.ToJsonForAttribute(), @category.NavigationParams.ToJsonForAttribute(), @category.Index.ToJsonForAttribute()); " 
                       onclick="javascript:displayStepsTwoAndThree(@category.NavigationUrl.ToJsonForAttribute(), @category.NavigationParams.ToJsonForAttribute(), @category.Index.ToJsonForAttribute()); 
                        " tabindex="0">@category.Name</a>*@
                    <a tabindex="0">@category.Title</a>
                </li>
                i++;
            }
        </ul>
    }
</div>